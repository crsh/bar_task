define inline_script StartNewBlock
	___run__
	# Import the widgets library
	from libopensesame import widgets
	
	# Create a form
	form = widgets.form(self.experiment, cols=[1], rows=[1],
		margins=(50,100,50,100), spacing=0)
	
	if 'block' in locals():
		pass
	else:
		block = 1
	
	if block == 1:
		rest = 'Das war die U+00DCbungsphase.'
	else:
		rest = ''
	
	# Create four widgets
	startblock = widgets.label(form, text='<span color="white", size="36">'+ rest + ' Gleich startet der %d. Experimentalblock.U+005C
	<br /><br /> DrU+00FCcken Sie die LEERTASTE, um zu beginnen.</span>' % block)
	
	# Add the widgets to the form. The position in the form is indicated as a
	# (column, row) tuple.
	form.set_widget(startblock, (0,0))
	
	block = block + 1
	
	form.render()
	__end__
	set _prepare ""
	set description "Executes Python code"

define inline_script response_2
	___run__
	#randomize fixation duration
	#Import random number generator
	import random
	random.seed()
	
	#define mouse object
	if self.get('rt') == 'None':
		#import expyriment mouse response
		from expyriment.io import Mouse
		my_mouse = Mouse(show_cursor=False, track_button_events=True, track_motion_events=None)
		#define 'wait_event': waits for mouse click and saves button and response time
		bla = my_mouse.wait_event(wait_button=True, buttons=[0,2], wait_motion=False, duration=1300)
		#'wait_event' tuple returens button click as first element, save this click!
		mouse_button_clicked = bla[0]
		#do not end bar presentation if there has been a click; always wait for 1300ms
		if mouse_button_clicked == None:
			rt = 1500
			self.sleep(random.randint(700,1200))
		elif mouse_button_clicked == 0 or mouse_button_clicked == 2:
			#'wait_event' tuple returens response click as fourth element, save this! Add 200ms because the bar presentation started 200ms before this script
			rt = int(bla[3]) + 200
			self.sleep(random.randint(700,1200)+(1300-rt))
		exp.set('mouse_button_clicked', mouse_button_clicked)
		exp.set('rt', rt)
	elif self.get('rt') != 'None':
		self.sleep(random.randint(2000,2500))
	__end__
	set _prepare ""
	set description "Executes Python code"

