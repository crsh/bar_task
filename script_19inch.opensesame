define notepad notepad
	__note__
	ORI = Change of the orientation of ONE bar, other bar does not change
	LUM = Change of the luminance of ONE ba, other bar does not change
	LOU = Change of the orientation AND the luminance of ONE bar, other bar does not change
	LOB = Change of the luminance of ONE bar, change of orientation of the other bar!
	
	For each combination of the pre change bars (which are 16), there are 2 possibilities to realize each 
	of the 4 conditions; thus conditions = 16*2*4 = 128
	__end__
	set description "A simple notepad to document your experiment. This plug-in does nothing."

define inline_script adjust_to_monitor
	___run__
	#color values to get the desired luminance values
	color_dark = '#737373' #corresponds to a luminance of 20cd/m^2 on the current screen, change for your screen!
	color_bright = '#ababab' #corresponds to a luminance of 45cd/m^2 on the current screen, change for your screen!
	
	exp.set('color_dark', color_dark)
	exp.set('color_bright', color_bright)
	
	##Insert three values! 
	
	#compute size stimuli etc. so that this experiment can be run on any monitor
	screen_cm = 36.5 ##insert  your value here
	screen_px = 1280 ##insert  your value here
	screen_distance = 60 ##insert  your value here
	
	
	import math
	
	# define tangez function so that seating distance can be related to stimuli size
	def tan(x):
	    import math
	    tangenz = math.tan((math.pi/180)*x)
	    return tangenz
	
	
	#bar stimuli correspond to a size of 0.56U+00B0 x 1.35U+00B0 of visual angle; if a participant is seated 60cm from the monitor this is in cm:
	bar_narrow_cm = 2*screen_distance*tan(0.56/2)
	bar_broad_cm = 2*screen_distance*tan(1.35/2)
	
	#distance of bars to center: 1.1U+00B0; in cm:
	distance_cm = 2*screen_distance*tan(1.1/2)
	
	#compute pixel sizes for the stimuli
	bar_broad_px = (screen_px/screen_cm)*bar_broad_cm
	bar_narrow_px = (screen_px/screen_cm)*bar_narrow_cm
	
	#compute pixels for distance between stimli and center
	distance_px = (screen_px/screen_cm)*distance_cm
	
	#compute position where the bars must be located
	vertical_x_left = -(distance_px + (bar_broad_px/2)-1)
	vertical_x_right = distance_px - (bar_broad_px/2)+1
	vertical_y = -((bar_narrow_px/2) + 1)
	
	horizontal_x_left = -(distance_px + (bar_narrow_px/2)-1)
	horizontal_x_right = distance_px - (bar_narrow_px/2)+1
	horizontal_y = -((bar_broad_px/2) + 1)
	
	
	#set the height and width of the bars to the experiment
	exp.set('bar_broad', bar_broad_px)
	exp.set('bar_narrow', bar_narrow_px)
	
	#set positioning of the bars to the experiment
	exp.set('vertical_x_left', vertical_x_left)
	exp.set('vertical_x_right', vertical_x_right)
	exp.set('vertical_y', vertical_y)
	exp.set('horizontal_x_left', horizontal_x_left)
	exp.set('horizontal_x_right', horizontal_x_right)
	exp.set('horizontal_y', horizontal_y)
	__end__
	set _prepare ""
	set description "Executes Python code"

